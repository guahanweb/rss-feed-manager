ts/1.1/";a:1:{s:7:"creator";a:1:{i:0;a:5:{s:4:"data";s:13:"Dean Garrison";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}}s:40:"http://purl.org/rss/1.0/modules/content/";a:1:{s:7:"encoded";a:1:{i:0;a:5:{s:4:"data";s:8807:"<div pw:image='' class='pw-widget pw-counter-vertical pw-horizontal' pw:url="http://freedomoutpost.com/2013/09/congress-drinking-job-part-reason-failing-american-people/" pw:title="Is Congress’ Drinking on the Job Part of the Reason They are Failing the American People?" >
